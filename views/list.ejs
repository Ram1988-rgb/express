<a href="/admin/logout">Logout</a>
<a href="/admin/adduser">Add User</a>
<table>
    <tr>
      <td>Name</td>
      <td>Email</td>
      <td>Phone</td>
    </tr>
    <%for(var i=0;i<data.length;i++){%>
    <tr>
      <td><%=data[i].name%></td>
      <td><%=data[i].email%></td>
      <td><%=data[i].phone%></td>
      <td><a href="/admin/user/edit/<%=data[i].id%>">Edit</a></td>
      <td><a href="/admin/user/delete/<%=data[i].id%>">Delete</a></td>
    </tr>
    <%}%>
</table>
